{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "CORS": {
    "PolicyName": "AllowAll"
  },
  "SPA": {
    "SourcePath": "Client"
  },
  "Compression": {
    "EnableForHttps": true,
    "MimeTypes": [
      "text/css",
      "text/xml",
      "text/html",
      "text/plain",
      "application/xml",
      "application/javascript"
    ]
  }
}
